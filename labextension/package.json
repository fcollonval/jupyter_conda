{
  "name": "jupyterlab_conda",
  "version": "2.2.4",
  "description": "Manage your conda environments from JupyterLab",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension",
    "conda",
    "mamba"
  ],
  "homepage": "https://github.com/mamba-org/jupyter_conda",
  "bugs": {
    "url": "https://github.com/mamba-org/jupyter_conda/issues"
  },
  "license": "BSD-3-Clause",
  "author": "Jupyter Development Team",
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "style/**/*.{css,eot,gif,html,jpg,json,png,svg,woff2,ttf}",
    "schema/*.json"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "repository": {
    "type": "git",
    "url": "https://github.com/mamba-org/jupyter_conda.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm run build:lib && jlpm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "build:lib": "tsc",
    "clean": "jlpm run clean:lib",
    "clean:all": "jlpm run clean:lib && jlpm run clean:labextension",
    "clean:labextension": "rimraf ../jupyter_conda/labextension",
    "clean:lib": "rimraf lib tsconfig.tsbuildinfo",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "install:extension": "jupyter labextension develop --overwrite .",
    "precommit": "pretty-quick --staged",
    "prepare": "jlpm run clean && jlpm run build",
    "prettier": "npx prettier --write \"**/*{.ts,.tsx,.js,.jsx,.css,.json,.md}\"",
    "test": "jest --coverage",
    "watch": "run-p watch:src watch:labextension",
    "watch:labextension": "jupyter labextension watch .",
    "watch:src": "tsc -w"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "@jupyterlab/application": "^3.0.0-rc.4",
    "@jupyterlab/apputils": "^3.0.0-rc.4",
    "@jupyterlab/coreutils": "^5.0.0-rc.4",
    "@jupyterlab/mainmenu": "^3.0.0-rc.4",
    "@jupyterlab/services": "^6.0.0-rc.4",
    "@jupyterlab/settingregistry": "^3.0.0-rc.4",
    "@jupyterlab/ui-components": "^3.0.0-rc.4",
    "@lumino/coreutils": "^1.5.3",
    "@lumino/signaling": "^1.4.3",
    "@lumino/widgets": "^1.14.0",
    "jupyterlab_toastify": "^4.1.2",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "react-virtualized": "^9.21.1",
    "semver": "^6.0.0||^7.0.0",
    "typestyle": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@jupyterlab/builder": "^3.0.0-rc.4",
    "@jupyterlab/testutils": "^3.0.0-rc.4",
    "@types/jest": "^25",
    "@types/react": "~16.9.48",
    "@types/react-dom": "~16.9.8",
    "@types/react-virtualized": "^9.21.4",
    "@types/semver": "^7.3.1",
    "@typescript-eslint/eslint-plugin": "^2.27.0",
    "@typescript-eslint/parser": "^2.27.0",
    "eslint": "^7.5.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "husky": "^0.14.0",
    "jest": "^25.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.0",
    "pretty-quick": "^1.6.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^25.0.0",
    "typescript": "~4.0.3"
  },
  "resolutions": {
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "@types/react": "~16.9.48",
    "@types/react-dom": "~16.9.8"
  },
  "sideEffects": [
    "style/*"
  ],
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema",
    "discovery": {
      "server": {
        "base": {
          "name": "jupyter_conda"
        },
        "managers": [
          "conda"
        ]
      }
    },
    "outputDir": "../jupyter_conda/labextension"
  },
  "publishConfig": {
    "access": "public"
  }
}